		function complete() {
            if($.trim($('[id$=erMSg]').html())!='') {
                $("html, body").animate({ scrollTop: 0 }, "fast");
            }
        }
        function addMandatorySign() {
            //alert('hii1');
            j$(".cust_required").each(function(obj) {
                //alert('hii');
                var pn = j$(this).closest('td').prev('th');
                j$(pn).addClass('label_required');
            });
        }


