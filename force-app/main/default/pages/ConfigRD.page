            bComponentPropertiesSaved = false;
        }





        // **************** on mouse over on section code
        function onMouseComponentsSelection() {
                if (bRatingRangePropertiesSaved == false) {
                    document.getElementById("toastMsgText").innerHTML = 'Unsaved Changes on "Rating & Range Properties"';
                    var elmnt = document.getElementById("notifyToast");
                    elmnt.style.display = 'inline-block';
                    elmnt.scrollIntoView();
                }

            }

            function onMouseCleaningTestProcessSelection() {
                if (bComponentPropertiesSaved == false) {
                    document.getElementById("toastMsgText").innerHTML = 'Unsaved Changes on "Components"';
                    var elmnt = document.getElementById("notifyToast");
                    elmnt.style.display = 'inline-block';
                    elmnt.scrollIntoView();
                }

            }


            /*########## after each section save @piyush*/

    function onRatingRangComplete(event) {
        event.value = 'Save & Continue';
        bRatingRangePropertiesSaved = true;
        document.getElementById("toastMsgText").innerHTML = '';
        var elmnt = document.getElementById("notifyToast");
        elmnt.style.display = 'none';
        return false;
    }


    function onComponentComplete(event) {
        event.value = 'Save & Continue';
        bComponentPropertiesSaved = true;
        document.getElementById("toastMsgText").innerHTML = '';
        var elmnt = document.getElementById("notifyToast");
        elmnt.style.display = 'none';
