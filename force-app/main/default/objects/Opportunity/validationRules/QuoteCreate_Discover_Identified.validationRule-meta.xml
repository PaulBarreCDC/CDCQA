<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QuoteCreate_Discover_Identified</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISNEW(),  TEXT(StageName) &lt;&gt; &apos;Discovery/Identified&apos;)</errorConditionFormula>
    <errorDisplayField>StageName</errorDisplayField>
    <errorMessage>Please start your Opportunity lifecycle with the first stage: Discover/Identified</errorMessage>
</ValidationRule>
