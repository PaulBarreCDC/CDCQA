<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>converted_pressure_setting_Constraint</fullName>
    <active>true</active>
    <errorConditionFormula>AND( Pressure_Setting__c &lt;= 0,  Relief_Flag__c = TRUE)</errorConditionFormula>
    <errorDisplayField>Pressure_Setting__c</errorDisplayField>
    <errorMessage>Pressure Setting: Please enter a positive Integer Value greater than 0.</errorMessage>
</ValidationRule>
