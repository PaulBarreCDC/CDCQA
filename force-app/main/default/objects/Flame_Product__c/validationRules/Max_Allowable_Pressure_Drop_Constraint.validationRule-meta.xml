<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Max_Allowable_Pressure_Drop_Constraint</fullName>
    <active>false</active>
    <description>This is a Mandatory field to be entered by the User.</description>
    <errorConditionFormula>AND(
ISBLANK( Max_Allowable_Pressure_Drop__c ),
Sizing_Flow_Area_flag__c=true)</errorConditionFormula>
    <errorDisplayField>Max_Allowable_Pressure_Drop__c</errorDisplayField>
    <errorMessage>Max. Allowable Pressure Drop: Please Enter a Value</errorMessage>
</ValidationRule>
