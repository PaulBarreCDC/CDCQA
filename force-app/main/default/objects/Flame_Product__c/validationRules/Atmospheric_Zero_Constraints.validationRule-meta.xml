<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Atmospheric_Zero_Constraints</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
Atmospheric_Pressure__c &lt;= 0,
Sizing_Flow_Area_flag__c=true
)</errorConditionFormula>
    <errorDisplayField>Atmospheric_Pressure__c</errorDisplayField>
    <errorMessage>Atmospheric Pressure: Please enter a positive Integer value greater than 0.</errorMessage>
</ValidationRule>
