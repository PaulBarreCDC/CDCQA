<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Del_Qty_Cannot_Exceed_QLI_Qty</fullName>
    <active>true</active>
    <errorConditionFormula>Quote_Line__r.Delivery_Quantity_Total__c +  Quantity__c &gt;  Quote_Line__r.SBQQ__Quantity__c</errorConditionFormula>
    <errorMessage>The input quantity and the sum of all delivery quantities exceeds the quote line item&apos;s quantity.</errorMessage>
</ValidationRule>
