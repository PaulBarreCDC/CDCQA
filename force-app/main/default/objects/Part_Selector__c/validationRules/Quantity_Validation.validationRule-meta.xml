<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quantity_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISNULL(Quantity__c), Product_Selection_Flag__c)</errorConditionFormula>
