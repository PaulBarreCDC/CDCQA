<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PartNumberBase_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
						NOT(ISNEW()),
LEN(ASSYPARTNOBASE__c )&lt;&gt;16
)</errorConditionFormula>
    <errorMessage>ASSYPARTNOBASE__c: Part Number must be less than 16 characters</errorMessage>
</ValidationRule>
