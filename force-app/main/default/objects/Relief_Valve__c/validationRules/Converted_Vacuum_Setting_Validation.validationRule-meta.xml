<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Converted_Vacuum_Setting_Validation</fullName>
    <active>false</active>
    <description>Value after conversion should be greater than 0.</description>
    <errorConditionFormula>AND(
(Converted_Vacuum_Setting__c  &lt;=  0),
(Section_A_Flag__c = TRUE))</errorConditionFormula>
    <errorDisplayField>Converted_Vacuum_Setting__c</errorDisplayField>
    <errorMessage>Converted Vacuum Setting must be greater than 0</errorMessage>
</ValidationRule>
